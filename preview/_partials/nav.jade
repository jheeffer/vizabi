-
  menuitems = [
    ['bubblechart.html', 'Bubble Chart'],
    ['mountainchart.html', 'Mountain Chart'],
    ['bubblemap.html', 'Bubble Map'],
    ['linechart.html', 'Line Chart'],
    ['popbyage.html', 'Pop By Age'],
    ['barchart.html', 'Bar Chart'],
    ['barrankchart.html', 'Bar Rank Chart'],
    ['donutchart.html', 'Donut Chart']
